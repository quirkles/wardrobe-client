{
  "name": "wardrobe-client",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "cross-env APP_ENV=local nuxt",
    "build:local": "cross-env APP_ENV=local nuxt-ts build",
    "build:test": "cross-env APP_ENV=test nuxt-ts build",
    "downloadSchemaJson:local": "apollo client:download-schema gqlSchema.json --config=apollo.config.js",
    "downloadSchemaGraphql:local": "apollo client:download-schema gqlSchema.graphql --config=apollo.config.js",
    "generateTypesFromQueries:local": "apollo client:codegen gqlSchemaTypes --target=typescript --includes='./queries/*.ts' --config=apollo.config.js",
    "generateFragmentTypes:local": "node utils/getFragmentTypesJson.js",
    "downloadSchemaJson:test": "cross-env APP_ENV=test apollo client:download-schema gqlSchema.json --config=apollo.config.js",
    "downloadSchemaGraphql:test": "cross-env APP_ENV=test apollo client:download-schema gqlSchema.graphql --config=apollo.config.js",
    "generateTypesFromQueries:test": "cross-env APP_ENV=test apollo client:codegen gqlSchemaTypes --target=typescript --includes='./queries/*.ts' --config=apollo.config.js",
    "generateFragmentTypes:test": "cross-env APP_ENV=test node utils/getFragmentTypesJson.js",
    "start": "nuxt-ts start",
    "export": "nuxt-ts export",
    "serve": "nuxt-ts serve",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint": "yarn lint:js",
    "test": "jest",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "^0.4.10",
    "@nuxtjs/apollo": "^4.0.1-rc.1",
    "@nuxtjs/fontawesome": "^1.1.2",
    "@nuxtjs/style-resources": "^1.0.0",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "aws-sdk": "^2.742.0",
    "buefy": "^0.9.3",
    "bulma": "^0.9.0",
    "dotenv": "^8.2.0",
    "graphql": "^15.3.0",
    "graphql-tag": "^2.10.3",
    "jsonwebtoken": "^8.5.1",
    "node-fetch": "^2.6.1",
    "node-sass": "^4.14.1",
    "nuxt": "^2.13.0",
    "nuxt-typed-vuex": "^0.1.19",
    "ramda": "^0.27.0",
    "rxjs": "^6.6.3",
    "typed-vuex": "^0.1.19",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/runtime-corejs2": "^7.10.3",
    "@fortawesome/free-brands-svg-icons": "^5.14.0",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@graphql-codegen/cli": "^1.15.4",
    "@graphql-codegen/fragment-matcher": "^1.15.4",
    "@graphql-codegen/introspection": "^1.17.8",
    "@graphql-codegen/typescript": "^1.15.4",
    "@nuxt/typescript-build": "^1.0.3",
    "@nuxtjs/eslint-config": "^3.0.0",
    "@nuxtjs/eslint-config-typescript": "^2.0.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/vue": "^5.3.19",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/ramda": "^0.27.7",
    "@types/faker": "^4.1.12",
    "@types/uuid": "^8.3.0",
    "@vue/test-utils": "^1.0.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "babel-preset-vue": "^2.0.2",
    "core-js": "2",
    "cross-env": "^7.0.2",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-graphql": "^4.0.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "faker": "^4.1.0",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "sass-loader": "^8.0.2",
    "ts-jest": "^26.1.0",
    "vue-jest": "^3.0.4"
  }
}
